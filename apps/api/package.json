{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run start:dev",
    "start:dev": "nodemon --watch \"src/**/*.ts\" --exec \"node -r ts-node/register -r tsconfig-paths/register src/main.ts\"",
    "start": "node dist/main.js",
    "build": "pnpm run clean && tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "test": "jest --config jest.config.ts --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@acme/shared-dto": "workspace:*",
    "@aws-sdk/client-s3": "^3.918.0",
    "@aws-sdk/s3-request-presigner": "^3.918.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.1.18",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^5.17.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "luxon": "^3.7.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/supertest": "^2.0.16",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.17.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.json prisma/seed.ts"
  }
}
